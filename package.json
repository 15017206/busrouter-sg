{
  "name": "busrouter-sg",
  "version": "1.0.0",
  "description": "Singapore Bus Routes Explorer",
  "devDependencies": {
    "@babel/core": "~7.10.3",
    "@babel/plugin-transform-runtime": "~7.10.3",
    "@babel/preset-env": "~7.10.3",
    "@mapbox/polyline": "~1.1.1",
    "@turf/buffer": "~5.1.5",
    "@turf/circle": "~6.0.1",
    "@turf/distance": "~6.0.1",
    "@turf/helpers": "~6.1.4",
    "@turf/length": "~6.0.2",
    "@turf/simplify": "~5.1.5",
    "autoprefixer": "~9.8.4",
    "cheerio": "~1.0.0-rc.3",
    "cpy-cli": "~3.1.1",
    "dotenv": "~8.2.0",
    "geokdbush": "~1.1.0",
    "got": "~11.3.0",
    "kdbush": "~3.0.0",
    "npm-run-all": "~4.1.5",
    "parcel-bundler": "~1.12.4",
    "parcel-plugin-json-url-loader": "~0.1.2",
    "postcss-import-url": "~5.1.0",
    "postcss-modules": "~3.1.0",
    "serve": "~11.3.2"
  },
  "author": "Lim Chee Aun",
  "license": "MIT",
  "scripts": {
    "watch": "parcel watch index.html bus-arrival/index.html bus-first-last/index.html visualization/index.html -d tmp --no-hmr --no-autoinstall",
    "serve": "serve -l 8888 tmp/",
    "start": "npm-run-all --parallel watch serve",
    "prebuild": "rm -rf dist && parcel --version",
    "build": "parcel build index.html bus-arrival/index.html bus-first-last/index.html visualization/index.html",
    "postbuild": "cpy favicon.ico dist && cpy well-known/* dist/.well-known && node tasks/generateHeaders",
    "prod": "npx serve dist/"
  },
  "dependencies": {
    "@babel/runtime": "~7.10.3",
    "@deck.gl/core": "~8.1.12",
    "@deck.gl/layers": "~8.1.12",
    "@deck.gl/mapbox": "~8.1.12",
    "cheap-ruler": "~3.0.1",
    "core-js": "~3.6.5",
    "fuse.js": "~6.3.1",
    "just-intersect": "~3.1.0",
    "lscache": "~1.3.0",
    "preact": "~10.4.4",
    "use-resize-observer": "~6.1.0"
  },
  "alias": {
    "react": "preact/compat"
  }
}
