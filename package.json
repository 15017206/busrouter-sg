{
  "name": "busrouter-sg",
  "version": "1.0.0",
  "description": "Singapore Bus Routes Explorer",
  "devDependencies": {
    "@babel/core": "~7.4.4",
    "@babel/plugin-proposal-class-properties": "~7.4.4",
    "@babel/plugin-transform-runtime": "~7.4.4",
    "@mapbox/polyline": "~1.1.0",
    "@turf/buffer": "~5.1.5",
    "@turf/circle": "~6.0.1",
    "@turf/distance": "~6.0.1",
    "@turf/helpers": "~6.1.4",
    "@turf/length": "~6.0.2",
    "@turf/simplify": "~5.1.5",
    "autoprefixer": "~9.5.1",
    "cheerio": "~1.0.0-rc.2",
    "cpy-cli": "~2.0.0",
    "dotenv": "~8.0.0",
    "geokdbush": "~1.1.0",
    "got": "~9.6.0",
    "kdbush": "~3.0.0",
    "npm-run-all": "~4.1.5",
    "parcel-bundler": "~1.12.3",
    "parcel-plugin-json-url-loader": "~0.1.2",
    "postcss-import-url": "~4.0.0",
    "postcss-modules": "~1.4.1",
    "serve": "~11.0.0"
  },
  "author": "Lim Chee Aun",
  "license": "MIT",
  "scripts": {
    "watch": "parcel watch index.html bus-arrival/index.html bus-first-last/index.html visualization/index.html -d tmp --no-hmr --no-autoinstall",
    "serve": "serve -l 1234 tmp/",
    "start": "npm-run-all --parallel watch serve",
    "prebuild": "rm -rf dist && parcel --version",
    "build": "parcel build index.html bus-arrival/index.html bus-first-last/index.html visualization/index.html",
    "postbuild": "cpy favicon.ico dist && cpy well-known/* dist/.well-known && node tasks/generateHeaders",
    "prod": "npx serve dist/"
  },
  "dependencies": {
    "@babel/runtime": "~7.4.4",
    "@deck.gl/core": "~7.0.5",
    "@deck.gl/layers": "~7.0.5",
    "@deck.gl/mapbox": "~7.0.5",
    "cheap-ruler": "~2.5.1",
    "fuse.js": "~3.4.4",
    "just-intersect": "~3.1.0",
    "lscache": "~1.3.0",
    "preact": "~10.0.0-beta.1"
  }
}
